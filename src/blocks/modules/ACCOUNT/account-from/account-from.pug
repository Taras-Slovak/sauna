  h1.account-from__title Настройка аккаунта

  .account-from__content 
    .account-from__card-group
      .account-from__card
        .account-from__card-top
          h3.account-from__card-title Имя          
          button.account-from__card-btn(type="button") Изменить
        .account-from__card-form
          .account-from__card-field
            label(for="firstName") Имя
            input(id="firstName" name="firstName" type="text" placeholder="Имя")
          .account-from__card-field
            label(for="secondName") Фамилия
            input(id="secondName" name="secondName" type="text" placeholder="Фамилия")
        a.account-from__card-save(href="account-personal.html") Сохранить
      .account-from__card-line

      .account-from__card
        .account-from__card-top
          h3.account-from__card-title Дата рождения          
          button.account-from__card-btn(type="button") Изменить
        .account-from__card-form
          .account-from__card-field
            label(for="birthDay") Дата рождения 
            input(id="birthDay" name="birthDay" type="text" placeholder="31.05.1980")
          
        a.account-from__card-save(href="account-personal.html") Сохранить
        
      .account-from__card-line

      .account-from__card
        .account-from__card-top
          h3.account-from__card-title Почта          
          button.account-from__card-btn(type="button") Изменить
        .account-from__card-form
          .account-from__card-field
            label(for="email") Почта
            input(id="email" name="email" type="mail" placeholder="example@example.ua")
          
        a.account-from__card-save(href="account-personal.html") Сохранить
      .account-from__card-line

      .account-from__card
        .account-from__card-top
          h3.account-from__card-title Номер телефона          
          button.account-from__card-btn(type="button") Изменить
        .account-from__card-form
          .account-from__card-field
            label(for="telephone") Номер телефона
            input(id="telephone" name="telephone" type="text" placeholder="0501234567")

        a.account-from__card-save(href="account-personal.html") Сохранить
      .account-from__card-line

