  .schedule-new__item
    h3.schedule-new__sub-title Бронирование

    .schedule-new__content
      .schedule-new__input-group

        .schedule-new__input
          .schedule-new__field
            label(for="date") Дата
            input.input-active(type="text" id="date" placeholder="26.06.2021")

        .schedule-new__input
          .schedule-new__field
            label(for="start-time") Время начала
            input(type="text" id="start-time" placeholder="16:00")

        .schedule-new__input
          .schedule-new__field
            label(for="duration") Длительность(час)
            input(type="text" id="duration" placeholder="4")

      .schedule-new__input-group.dynamic-content
        .schedule-new__input
          .schedule-new__field
            label(for="persons") Человек
            input(type="text" id="persons" placeholder="14")
        
        .schedule-new__field
          p Время окончания 18:00 
      
      .schedule-new__line

